{
  "name": "slo-ui",
  "version": "0.0.1",
  "description": "slo ui",
  "scope": "@slo-ui",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/slo-ui/slo-ui.esm.js",
  "exports": {
    ".": {
      "import": "./dist/slo-ui/slo-ui.esm.js",
      "require": "./dist/slo-ui/slo-ui.cjs.js"
    },
    "./slo-button": {
      "import": "./dist/components/slo-button.js",
      "types": "./dist/components/slo-button.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/salehtl/slo-ui.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@stencil/core": "4.22.3",
    "@types/jest": "^29.5.6",
    "@types/node": "^16.18.11",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "puppeteer": "^21.9.0"
  },
  "license": "UNLICENSED"
}
